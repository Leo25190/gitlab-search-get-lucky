{
  "manifest_version": 3,
  "name": "GitLab Search Get Lucky",
  "version": "1.0.1",
  "description": "Press Enter in GitLab search to instantly jump to the first result",
  
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  
  "action": {
    "default_popup": "popup.html"
  },
  
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "js": ["dist/content.js"]
    }
  ]
}