{
  "manifest_version": 3,
  "name": "GitLab Search Get Lucky",
  "version": "1.0.0",
  "description": "Instantly navigate to the first search result in GitLab with Enter key. Prioritizes projects for faster workflow.",
  
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    },
    "default_title": "GitLab Search Get Lucky - How to use"
  },
  
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "js": ["dist/content.js"],
      "run_at": "document_end"
    }
  ],
  
  "permissions": [],
  
  "host_permissions": ["*://*/*"],
  
  "author": "Your Name",
  "homepage_url": "https://github.com/leo25190",
  
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  }
}