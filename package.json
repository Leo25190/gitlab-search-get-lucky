  "homepage": "https://github.com/leo25190/gitlab-search-get-lucky",
  "repository": {
    "type": "git",
    "url": "https://github.com/leo25190/gitlab-search-get-lucky.git"
  },
  "bugs": {
    "url": "https://github.com/leo25190/gitlab-search-get-lucky/issues"
  },
  "devDependencies": {
    "typescript": "^5.0.0"
  }
