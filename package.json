{
  "name": "gitlab-search-get-lucky",
  "version": "1.0.0",
  "description": "Chrome extension for instant GitLab search navigation",
  "main": "dist/content.js",
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
    "clean": "rm -rf dist/",
    "package": "npm run clean && npm run build && zip -r gitlab-search-get-lucky.zip . -x node_modules/\\* .git/\\* \\*.ts tsconfig.json package*.json"
  },
  "keywords": [
    "gitlab",
    "search",
    "navigation",
    "productivity",
    "chrome-extension",
    "developer-tools"
  ],
